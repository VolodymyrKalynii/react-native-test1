{
  "compilerOptions": {
    "baseUrl": "src",
    "baseDir": "src",
    "paths": {
      "red/*": [
        "redux/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
